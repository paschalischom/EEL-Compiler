/* Switch statement test using an input statement. Choose a
   number between 1 and 12 and it will be printed on screen.    */
// Expected Results: month
program switchStat
    declare month enddeclare
    input month;
    switch month
        case 1: print 1
        case 2: print 2
        case 3: print 3
        case 4: print 4
        case 5: print 5
        case 6: print 6
        case 7: print 7
        case 8: print 8
        case 9: print 9
        case 10: print 10
        case 11: print 11
        case 12: print 12
    endswitch
endprogram